{
  "branches": [
    "master",
    "alpha",
    "beta"
  ],
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    [
      "@semantic-release/exec",
      {
        "prepareCmd": "semantic-release-rust prepare ${nextRelease.version}"
      }
    ],
    [
      "@semantic-release/exec",
      {
        "prepareCmd": "cargo build --release --target x86_64-unknown-linux-gnu"
      }
    ],
    "@semantic-release/npm",
    [
      "@semantic-release/github",
      {
        "assets": [
          {
            "path": "dist/typescript-tools-x86_64-unknown-linux-gnu.tar.gz",
            "label": "x86_64-unknown-linux-gnu"
          },
          {
            "path": "dist/typescript-tools-x86_64-apple-darwin.tar.gz",
            "label": "x86_64-apple-darwin"
          }
        ]
      }
    ]
  ]
}
